.block9
  width 100%
  height auto
  background-color #f0f5f8

  .block9-content
    width 100%
    height auto
    justify-content center
    align-items center
    flex-direction column
    padding-bottom 48px
    box-sizing border-box

    .insta-blocks
      width 100%
      height auto

      .insta-block
        width 296px
        height 425px
        box-shadow 0 5px 15px rgba(0, 0, 0, 0.05)
        border-radius 4px
        border 1px solid #f6f6f6
        background-color #ffffff